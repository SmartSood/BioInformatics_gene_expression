datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String
  degree  String
  field    String
  university String
  graduationYear Int
  password String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  
}